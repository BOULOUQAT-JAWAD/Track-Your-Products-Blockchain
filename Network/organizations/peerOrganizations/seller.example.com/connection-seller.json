{
    "name": "test-network-${ORG_L}",
    "version": "1.0.0",
    "client": {
        "organization": "Seller",
        "connection": {
            "timeout": {
                "peer": {
                    "endorser": "300"
                }
            }
        }
    },
    "organizations": {
        "OrgSeller": {
            "mspid": "SellerMSP",
            "peers": [
                "peer0.${ORG_L}.example.com"
            ],
            "certificateAuthorities": [
                "ca.${ORG_L}.example.com"
            ]
        }
    },
    "peers": {
        "peer0.orgSeller.example.com": {
            "url": "grpcs://localhost:11051",
            "tlsCACerts": {
                "pem": ""
            },
            "grpcOptions": {
                "ssl-target-name-override": "peer0.${ORG_L}.example.com",
                "hostnameOverride": "peer0.${ORG_L}.example.com"
            }
        }
    },
    "certificateAuthorities": {
        "ca.orgSeller.example.com": {
            "url": "https://localhost:11054",
            "caName": "ca-${ORG_L}",
            "tlsCACerts": {
                "pem": [""]
            },
            "httpOptions": {
                "verify": false
            }
        }
    }
}
